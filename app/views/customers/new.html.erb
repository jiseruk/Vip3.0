<h1>New customer</h1>

<% form_for(@customer) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nickname %><br />
    <%= f.text_field :nickname %>
  </p>
  <p>
    <%= f.label :points %><br />
    <%= f.text_field :points %>
  </p>
  <p>
    <%= f.label :qty_calif %><br />
    <%= f.text_field :qty_calif %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', customers_path %>