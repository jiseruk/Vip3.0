<h1>Listing ship_methods</h1>

<table>
  <tr>
    <th>Description</th>
  </tr>

<% @ship_methods.each do |ship_methods| %>
  <tr>
    <td><%=h ship_methods.description %></td>
    <td><%= link_to 'Show', ship_methods %></td>
    <td><%= link_to 'Edit', edit_ship_methods_path(ship_methods) %></td>
    <td><%= link_to 'Destroy', ship_methods, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New ship_methods', new_ship_methods_path %>